    "devDependencies": {
        "@vue/cli-plugin-babel": "^4.0.0",
        "@vue/cli-plugin-eslint": "^4.0.0",
        "@vue/cli-service": "^4.0.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "vue-template-compiler": "^2.6.10",
        "clean-webpack-plugin": "^0.1.19",
        "webpack": "^4.16.3",
        "webpack-cli": "^3.0.8",
        "webpack-dev-server": "^3.1.5"
